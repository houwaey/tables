CREATE TABLE PORTAL.ACCOUNT_HIT
(
  ID                   NUMBER,
  TIMESTAMP            DATE                     DEFAULT SYSDATE,
  MSISDN               VARCHAR2(100 BYTE),
  FIRSTNAME            VARCHAR2(100 BYTE),
  LASTNAME             VARCHAR2(100 BYTE),
  MIDDLENAME           VARCHAR2(100 BYTE),
  DATEOFBIRTH          DATE,
  GENDER               VARCHAR2(100 BYTE),
  NATIONALITY          VARCHAR2(100 BYTE),
  PLACEOFBIRTHADDRESS  VARCHAR2(100 BYTE),
  PLACEOFBIRTHCITY     VARCHAR2(100 BYTE),
  PLACEOFBIRTHREGION   VARCHAR2(100 BYTE),
  PLACEOFBIRTHCOUNTRY  VARCHAR2(100 BYTE),
  PLACEOFBIRTHZIPCODE  NUMBER,
  RESIDENTADDRESS      VARCHAR2(100 BYTE),
  RESIDENTCITY         VARCHAR2(100 BYTE),
  RESIDENTREGION       VARCHAR2(100 BYTE),
  RESIDENTCOUNTRY      VARCHAR2(100 BYTE),
  RESIDENTZIPCODE      NUMBER,
  PERMANENTADDRESS     VARCHAR2(100 BYTE),
  PERMANENTCITY        VARCHAR2(100 BYTE),
  PERMANENTREGION      VARCHAR2(100 BYTE),
  PERMANENTCOUNTRY     VARCHAR2(100 BYTE),
  PERMANENTZIPCODE     NUMBER,
  BILLINGADDRESS       VARCHAR2(100 BYTE),
  BILLINGCITY          VARCHAR2(100 BYTE),
  BILLINGREGION        VARCHAR2(100 BYTE),
  BILLINGCOUNTRY       VARCHAR2(100 BYTE),
  BILLINGZIPCODE       NUMBER,
  WORKADDRESS          VARCHAR2(100 BYTE),
  WORKCITY             VARCHAR2(100 BYTE),
  WORKREGION           VARCHAR2(100 BYTE),
  WORKCOUNTRY          VARCHAR2(100 BYTE),
  WORKZIPCODE          NUMBER,
  NATUREOFWORK         VARCHAR2(100 BYTE),
  REASONS              VARCHAR2(100 BYTE),
  HITSTATUS            VARCHAR2(10 BYTE),
  CASE                 NUMBER,
  ACCOUNTNUMBER        VARCHAR2(100 BYTE)
)
TABLESPACE DATA
RESULT_CACHE (MODE DEFAULT)
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
            FLASH_CACHE      DEFAULT
            CELL_FLASH_CACHE DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;
